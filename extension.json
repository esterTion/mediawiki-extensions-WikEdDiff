	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "WikEdDiff/modules"
	},
	"Hooks": {
		"GetDifferenceEngine": "WikEdDifferenceEngine::onGetDifferenceEngine"
	},
	"manifest_version": 2
